<?xml version="1.1" encoding="UTF-8"?>
<ehcache updateCheck="false" name="defaultCache">


	<cache name="serviceCache" maxEntriesLocalHeap="2000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="0" overflowToDisk="false"
		maxElementsInMemory="10000" statistics="true">
	</cache>

	<cache name="menucache" maxEntriesLocalHeap="2000" eternal="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="0" overflowToDisk="false"
		maxElementsInMemory="10000" statistics="true">
	</cache>

	<diskStore path="java.io.tmpdir/ehcache" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="600" overflowToDisk="true"
		maxEntriesLocalDisk="100000" />


	<cache name="org.hibernate.cache.StandardQueryCache"
		maxEntriesLocalHeap="5" eternal="false" timeToLiveSeconds="120" />

	<cache name="org.hibernate.cache.UpdateTimestampsCache"
		maxEntriesLocalHeap="5000" eternal="true" />



	<cache name="com.tongna.iowl.data.entity.Bid"
		maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true"
		maxEntriesLocalDisk="10000" />
</ehcache>
